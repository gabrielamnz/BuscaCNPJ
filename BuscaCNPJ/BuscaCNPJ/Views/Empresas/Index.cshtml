        <td>
            @Html.DisplayFor(modelItem => item.CEP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Situacao)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { cnpj=item.CNPJ }) |
            @Html.ActionLink("Details", "Details", new { cnpj=item.CNPJ }) |
            @Html.ActionLink("Delete", "Delete", new { cnpj=item.CNPJ })
        </td>
    </tr>
}

</table>
